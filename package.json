{
    "name": "olympus-general",
    "version": "1.0.0",
    "description": "An api to centralize Olympus services",
    "author": "Wellington Reis <wellington.reis@blip.ai>",
    "keywords": [
        "node"
    ],
    "main": "src/server.js",
    "files": [
        "src"
    ],
    "scripts": {
        "coverage": "npx jest --colors --coverage",
        "jest": "npx jest --colors --verbose",
        "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
        "start": "node src/server.js",
        "test": "npm run lint && npm run jest"
    },
    "jest": {
        "testResultsProcessor": "jest-sonar-reporter",
        "collectCoverage": true,
        "testTimeout": 10000
    },
    "dependencies": {
        "blip-sdk": "^8.0.0",
        "bunyan": "^1.8.15",
        "bunyan-seq": "^0.4.0",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-mung": "^0.5.1",
        "express-unless": "^0.5.0",
        "http-status-codes": "^1.4.0",
        "lime-js": "^3.0.1",
        "lime-transport-websocket": "^2.0.3",
        "swagger-jsdoc": "^4.3.2",
        "swagger-ui-express": "^4.6.3",
        "uuid": "^7.0.3",
        "yup": "^0.28.5"
    },
    "devDependencies": {
        "@types/jest": "^24.9.1",
        "coveralls": "^3.1.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-prettier": "^3.4.1",
        "jest": "^24.9.0",
        "jest-sonar-reporter": "^2.0.0",
        "prettier": "^1.19.1",
        "superagent": "^5.3.1",
        "supertest": "^4.0.2"
    },
    "license": "MIT"
}
